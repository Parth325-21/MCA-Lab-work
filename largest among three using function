#include <stdio.h>
int largest(int,int,int);
void main() {
    int num1,num2,num3;
    printf("enter the value of three numbers");
    scanf("%d %d %d",&num1,&num2,&num3);
 largest(num1,num2,num3);
}
int largest(int n1,int n2,int n3)
{
    if(n1>n2 && n1>n3)
    {
        printf("the greater number is %d",n1);
    }
    else if(n2>n1 && n2>n3)
    {
        
        printf("the greater number is %d",n2);
    }
    else if(n3>n1 && n3>n2)
    {
        printf("the greater number is %d",n3);
    }
    else 
    {
        printf("all number are equal");
    }
    return 0;
}
