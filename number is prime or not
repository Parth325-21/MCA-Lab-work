#include<stdio.h>
#include<math.h>
void main()
{
    int num,i,flag=0;
    printf("enter the number");
    scanf("%d",&num);
    for(i=2;i<=sqrt(num);i++)
    {
        if(num%i==0)
        {
            flag = 1;
            break;
        }
    }
    if(flag==1)
    {
        printf("Not prime");
    }
    else
    {
        printf("Number is Prime");
    }
}
